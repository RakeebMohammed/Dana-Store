<div class="cart-section mt-5 mb-5">
		<div class="container-fluid">
			<div class="row">
				<div class="col-lg-12 col-md-12">
					<p class="text-success text-center" id="successs"></p>
					<div class="cart-table-wrap">
						<table class="cart-table">
							
							<tbody>
								{{#each result}}
								<tr class="table-body-row">
									    <td class="product-name">
                <img  style="width:50px;height:50px;" src='/product-images/{{this.orderproducts._id}}.jpg' alt=""> </td>
           
									<td class="product-name">{{this.orderproducts.name}}</td>
                                    <td class="product-name">{{this.orderproducts.price}}</td>
									
									{{!-- <td class="product-name">{{this.subTotal}}</td> --}}
									 <td class="product-price">{{this.quantity}}</td>
                                  
								    <td class="product-name">{{this.PaymentMethod}}</td>
    
	                               

									
								</tr>
								 {{/each}}
							</tbody>
						</table>
					</div>
				</div>
            </div>
			
        </div>
</div>
